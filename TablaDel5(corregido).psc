Proceso TablaDel5
	Definir resultado, multiplicador Como Entero;
	resultado <- 0;
    multiplicador <- 1;
	
    Mientras multiplicador <= 10 Hacer
        resultado <- 5 * multiplicador;
        Escribir "5 x ", multiplicador, " = ", resultado;
        multiplicador <- multiplicador + 1;
    FinMientras
FinProceso
